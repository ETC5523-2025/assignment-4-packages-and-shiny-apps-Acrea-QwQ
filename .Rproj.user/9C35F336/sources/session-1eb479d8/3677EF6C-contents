set.seed(35285397)

year <- 1990:2023
n <- length(year)

# simulate household disposable income data
income <- 100 + (year - 1990) * 6 + rnorm(n, 0, 6)

# simulate detached house prices data
house_price <- 100 * exp(0.06 * (year - 1990)) + rnorm(n, 0, 30)

# out put data set 
housepr_income <- data.frame(year, income, house_price)

